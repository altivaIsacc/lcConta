<?xml version="1.0" standalone="yes" ?>
<xs:schema id="DataSet1" targetNamespace="http://www.tempuri.org/DataSet1.xsd" xmlns:mstns="http://www.tempuri.org/DataSet1.xsd"
	xmlns="http://www.tempuri.org/DataSet1.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="DataSet1" msdata:IsDataSet="true" msdata:Locale="es-CR">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Cuentas_bancarias">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Cuenta" type="xs:string" />
							<xs:element name="Codigo_banco" type="xs:long" />
							<xs:element name="NombreCuenta" type="xs:string" />
							<xs:element name="Id_CuentaBancaria" type="xs:int" />
							<xs:element name="Descripcion" type="xs:string" />
							<xs:element name="MonedaNombre" type="xs:string" />
							<xs:element name="Saldo" msdata:ReadOnly="true" type="xs:double" minOccurs="0" />
							<xs:element name="tipoCuenta" type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Cheques">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Portador" type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ListCheques">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Id_Cheque" type="xs:int" minOccurs="0" />
							<xs:element name="Portador" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Proveedores">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CodigoProv" type="xs:int" />
							<xs:element name="Cedula" type="xs:string" />
							<xs:element name="Nombre" type="xs:string" />
							<xs:element name="Contacto" type="xs:string" />
							<xs:element name="Telefono_Cont" type="xs:string" />
							<xs:element name="Observaciones" type="xs:string" />
							<xs:element name="Telefono1" type="xs:string" />
							<xs:element name="Telefono2" type="xs:string" />
							<xs:element name="Fax1" type="xs:string" />
							<xs:element name="Fax2" type="xs:string" />
							<xs:element name="Email" type="xs:string" />
							<xs:element name="Direccion" type="xs:string" />
							<xs:element name="MontoCredito" type="xs:double" />
							<xs:element name="Plazo" type="xs:int" />
							<xs:element name="CostoTotal" type="xs:boolean" />
							<xs:element name="ImpIncluido" type="xs:boolean" />
							<xs:element name="PedidosMes" type="xs:int" />
							<xs:element name="Utilidad_Inventario" type="xs:double" />
							<xs:element name="Utilidad_Fija" type="xs:boolean" />
							<xs:element name="CuentaContable" type="xs:string" minOccurs="0" />
							<xs:element name="DescripcionCuentaContable" type="xs:string" minOccurs="0" />
							<xs:element name="TourOperador" type="xs:boolean" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="vChequesDep">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Id_Cheque" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />
							<xs:element name="Num_Cheque" type="xs:long" />
							<xs:element name="NombreCuenta" type="xs:string" />
							<xs:element name="Id_CuentaBancaria" type="xs:int" />
							<xs:element name="Fecha" type="xs:dateTime" />
							<xs:element name="Portador" type="xs:string" />
							<xs:element name="Monto" type="xs:double" />
							<xs:element name="Conciliado" type="xs:boolean" />
							<xs:element name="Anulado" type="xs:boolean" />
							<xs:element name="Observaciones" type="xs:string" />
							<xs:element name="Ced_Usuario" type="xs:string" />
							<xs:element name="Cuenta_Destino" type="xs:string" />
							<xs:element name="Tipo" type="xs:string" />
							<xs:element name="CodigoMoneda" type="xs:int" />
							<xs:element name="TipoCambio" type="xs:double" />
							<xs:element name="InfoDep" type="xs:string" />
							<xs:element name="InfoBanco" type="xs:string" />
							<xs:element name="MontoDep" minOccurs="0" type="xs:double" />
							<xs:element name="MonedaDep" type="xs:int" minOccurs="0" />
							<xs:element name="MonedaStr" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Bancos">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Codigo_banco" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />
							<xs:element name="Descripcion" type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Cuentas_bancarias1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Cuenta" type="xs:string" />
							<xs:element name="Codigo_banco" type="xs:long" />
							<xs:element name="tipoCuenta" type="xs:string" />
							<xs:element name="NombreCuenta" type="xs:string" />
							<xs:element name="SaldoInicial" type="xs:double" />
							<xs:element name="CuentaContable" type="xs:string" />
							<xs:element name="ChequeInicial" type="xs:int" />
							<xs:element name="ChequeFinal" type="xs:int" />
							<xs:element name="Cod_Moneda" type="xs:int" />
							<xs:element name="Id_CuentaBancaria" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="NombreCuentaContable" type="xs:string" />
							<xs:element name="Fecha" type="xs:dateTime" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="DataSet1Key1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Proveedores" />
			<xs:field xpath="mstns:CodigoProv" />
		</xs:unique>
		<xs:unique name="DataSet1Key2" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Bancos" />
			<xs:field xpath="mstns:Codigo_banco" />
		</xs:unique>
		<xs:unique name="DataSet1Key3" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Cuentas_bancarias1" />
			<xs:field xpath="mstns:Id_CuentaBancaria" />
		</xs:unique>
	</xs:element>
</xs:schema>